<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/toolbar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        app:title="CONTAINER MOD" />

    <LinearLayout
        android:orientation="vertical"
        android:paddingBottom="8.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingStart="?listPreferredItemPaddingLeft"
        android:paddingEnd="?listPreferredItemPaddingRight">

        <TextView
            android:textAppearance="?textAppearanceSubtitle1"
            android:textStyle="bold"
            android:ellipsize="end"
            android:textSize="23sp"
            android:paddingBottom="8.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Games (3)" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:gravity="center_vertical">

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_gravity="center"
                android:id="@+id/icon"
                android:src="@drawable/icon"
                android:layout_width="@dimen/game_icon_size_normal"
                android:layout_height="@dimen/game_icon_size_normal"
                app:shapeAppearance="@style/Theme.CheatUIDev.ShapeAppearanceOverlay.Circle" />

            <Space
                android:layout_gravity="center_vertical"
                android:layout_width="10.0dip"
                android:layout_height="0.0dip" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
                    android:id="@+id/title"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="PUBG Mobile Global"
                    android:maxLines="1"
                    android:singleLine="true" />

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:id="@+id/vrglobal"
                    android:text="PUBG Mobile 1.9.0 Global\n1.9.0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <com.google.android.material.button.MaterialButton
                android:textSize="10sp"
                android:text="CLONE"
                android:id="@+id/InstallGlobal"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.Button.OutlinedButton" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:gravity="center_vertical">

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_gravity="center"
                android:src="@drawable/korea"
                android:layout_width="@dimen/game_icon_size_normal"
                android:layout_height="@dimen/game_icon_size_normal"
                app:shapeAppearance="@style/Theme.CheatUIDev.ShapeAppearanceOverlay.Circle" />

            <Space
                android:layout_gravity="center_vertical"
                android:layout_width="10.0dip"
                android:layout_height="0.0dip" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="PUBG Mobile Korea"
                    android:maxLines="1"
                    android:singleLine="true" />

                <TextView
                    android:id="@+id/vrkorea"
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:text="PUBG Mobile 1.9.0 Korea\n1.9.0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <com.google.android.material.button.MaterialButton
                android:textSize="10sp"
                android:text="CLONE"
                android:id="@+id/installkorea"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.Button.OutlinedButton" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:gravity="center_vertical">

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_gravity="center"
                android:src="@drawable/india"
                android:layout_width="@dimen/game_icon_size_normal"
                android:layout_height="@dimen/game_icon_size_normal"
                app:shapeAppearance="@style/Theme.CheatUIDev.ShapeAppearanceOverlay.Circle" />

            <Space
                android:layout_gravity="center_vertical"
                android:layout_width="10.0dip"
                android:layout_height="0.0dip" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="PUBG Mobile India"
                    android:maxLines="1"
                    android:singleLine="true" />

                <TextView
                    android:id="@+id/vrindia"
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:text="PUBG Mobile 1.9.0 India\n1.9.0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <com.google.android.material.button.MaterialButton
                android:textSize="10sp"
                android:text="CLONE"
                android:id="@+id/InstallBgmi"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.Button.OutlinedButton" />

        </LinearLayout>

        <TextView
            android:textAppearance="?textAppearanceSubtitle1"
            android:textStyle="bold"
            android:ellipsize="end"
            android:textSize="23sp"
            android:paddingTop="8.0dip"
            android:paddingBottom="8.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Social Apps (2)" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:gravity="center_vertical">

            <FrameLayout
                android:layout_width="50.0dip"
                android:layout_height="50.0dip">

                <com.google.android.material.imageview.ShapeableImageView
                    android:layout_gravity="center"
                    android:src="@drawable/fb"
                    android:layout_width="@dimen/game_icon_size_normal"
                    android:layout_height="@dimen/game_icon_size_normal"
                    app:shapeAppearance="@style/Theme.CheatUIDev.ShapeAppearanceOverlay.Circle" />

                <ProgressBar
                    android:id="@+id/downfacebook"
                    android:layout_gravity="center"
                    android:padding="0.0dip"
                    android:layout_width="50.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="0.0dip"
                    android:indeterminateOnly="false"
                    android:indeterminateDrawable="@drawable/progress_game_download_indeterminate"
                    android:progressDrawable="@drawable/progress_game_download"
                    style="?android:progressBarStyle" />

            </FrameLayout>

            <Space
                android:layout_gravity="center_vertical"
                android:layout_width="10.0dip"
                android:layout_height="0.0dip" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Facebook"
                    android:maxLines="1"
                    android:singleLine="true" />

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:text="Facebook"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:text="363.0.0.30.112"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <com.google.android.material.button.MaterialButton
                android:textSize="10sp"
                android:text="CLONE"
                android:id="@+id/installfacebook"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.Button.OutlinedButton" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:gravity="center_vertical">

            <FrameLayout
                android:layout_width="50.0dip"
                android:layout_height="50.0dip">

                <com.google.android.material.imageview.ShapeableImageView
                    android:layout_gravity="center"
                    android:src="@drawable/tw"
                    android:layout_width="@dimen/game_icon_size_normal"
                    android:layout_height="@dimen/game_icon_size_normal"
                    app:shapeAppearance="@style/Theme.CheatUIDev.ShapeAppearanceOverlay.Circle" />

                <ProgressBar
                    android:id="@+id/downtwitter"
                    android:layout_gravity="center"
                    android:layout_width="60dp"
                    android:layout_height="60.0dip"
                    android:indeterminateOnly="false"
                    android:indeterminateDrawable="@drawable/progress_game_download_indeterminate"
                    android:progressDrawable="@drawable/progress_game_download"
                    style="?android:progressBarStyle" />

            </FrameLayout>

            <Space
                android:layout_gravity="center_vertical"
                android:layout_width="10.0dip"
                android:layout_height="0.0dip" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Twitter"
                    android:maxLines="1"
                    android:singleLine="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="19dp"
                    android:text="Twitter"
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption" />

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:text="9.39.0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <com.google.android.material.button.MaterialButton
                android:textSize="10sp"
                android:id="@+id/installtwitter"
                android:text="CLONE"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.Button.OutlinedButton" />

        </LinearLayout>

        <TextView
            android:textAppearance="?textAppearanceSubtitle1"
            android:textStyle="bold"
            android:ellipsize="end"
            android:textSize="23sp"
            android:paddingTop="8.0dip"
            android:paddingBottom="8.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Other Apps (1)" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:gravity="center_vertical">

            <FrameLayout
                android:layout_width="50.0dip"
                android:layout_height="50.0dip">

                <com.google.android.material.imageview.ShapeableImageView
                    android:layout_gravity="center"
                    android:src="@drawable/fb"
                    android:layout_width="@dimen/game_icon_size_normal"
                    android:layout_height="@dimen/game_icon_size_normal"
                    app:shapeAppearance="@style/Theme.CheatUIDev.ShapeAppearanceOverlay.Circle" />

                <ProgressBar
                    android:id="@+id/downfacebook"
                    android:layout_gravity="center"
                    android:padding="0.0dip"
                    android:layout_width="50.0dip"
                    android:layout_height="50.0dip"
                    android:layout_margin="0.0dip"
                    android:indeterminateOnly="false"
                    android:indeterminateDrawable="@drawable/progress_game_download_indeterminate"
                    android:progressDrawable="@drawable/progress_game_download"
                    style="?android:progressBarStyle" />

            </FrameLayout>

            <Space
                android:layout_gravity="center_vertical"
                android:layout_width="10.0dip"
                android:layout_height="0.0dip" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="GG"
                    android:maxLines="1"
                    android:singleLine="true" />

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:text="gg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:textAppearance="@style/TextAppearance.MaterialComponents.Caption"
                    android:text="363.0.0.30.112"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <com.google.android.material.button.MaterialButton
                android:textSize="10sp"
                android:text="CLONE"
                android:id="@+id/installgg"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                style="@style/Widget.MaterialComponents.Button.OutlinedButton" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/telegram"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_weight="1">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:src="@drawable/ic_whatshot"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sameer Arora"
            android:textSize="25sp"
            android:textStyle="bold"
            android:textColor="#000000"/>

    </LinearLayout>

</LinearLayout>